{
	"root": true,
	"extends": [
		"wikimedia/client-es5",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"jQuery": false
	},
	"rules": {
		"no-implicit-globals": "warn",
		"one-var": "warn",
		"vars-on-top": "warn",
		"no-unused-vars": "warn",
		"no-jquery/variable-pattern": "warn"
	}
}
